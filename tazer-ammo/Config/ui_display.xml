<khaine>
	<!-- Adding Tazer Ammo to ui_display so stats display -->
	<append xpath="/ui_display_info/item_display">
		<item_display_info display_type="ammoBulletTazer" display_group="groupAmmo">
			<display_entry name="EntityDamage" display_type="Decimal1" title_key="statEntityDamageRanged" tags="perkGunslinger,perkBoomstick,perkDeadEye,perkMachineGunner,perkBookAutoWeapons,perkTurrets,perkArchery,admin"/>
			<display_entry name="BlockDamage" display_type="Decimal1" title_key="statBlockDamage" tags="perkGunslinger,perkBoomstick,perkDeadEye,perkMachineGunner,perkBookAutoWeapons,perkTurrets,perkArchery,admin"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
	</append>
</khaine>